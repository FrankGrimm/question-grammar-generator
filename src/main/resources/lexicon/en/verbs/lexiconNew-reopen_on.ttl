@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_complete ;
  lemon:entry    :in .

:to_complete a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_complete ;
  lemon:otherForm      :form_complete_past ;
  lemon:synBehavior    :complete_frame ;
  lemon:sense          :complete_ontomap ;
  lemon:sense          :complete_ontomap_sense2 ;
  lemon:sense          :complete_condition_sense3 .

:form_complete a           lemon:Form ;
  lemon:writtenRep     "reopen"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_complete_past a lemon:Form ;
  lemon:writtenRep  "reopened"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .



:complete_frame a                  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :complete_subj ;
  lexinfo:prepositionalAdjunct :complete_obj .

:complete_ontomap a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :complete_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/reopeningDate> ;
  lemon:subjOfProp  :complete_obj ;
  lemon:objOfProp   :complete_subj ;
  lemon:condition   :complete_condition .


:complete_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:complete_ontomap_sense2 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :complete_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/reopeningDate> ;
  lemon:subjOfProp  :complete_obj ;
  lemon:objOfProp   :complete_subj ;
  lemon:condition   :complete_condition_sense2 .


:complete_condition_sense2 a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .

:complete_ontomap_sense3 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :complete_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/reopeningDate> ;
  lemon:subjOfProp  :complete_obj ;
  lemon:objOfProp   :complete_subj ;
  lemon:condition   :complete_condition_sense3 .


:complete_condition_sense3 a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:complete_obj lemon:marker :in .

## Prepositions ##

:in a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

