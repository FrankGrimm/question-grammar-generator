@prefix : <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon: <http://lemon-model.net/lemon#> .

@base <http://localhost:8080#> .

:lexicon_en a lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry :founder_of ;
  lemon:entry :of .

:founder_of a lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm :founder_form ;
  lemon:synBehavior :founder_of_nounpp ;
  lemon:sense :founder_sense_ontomap .

:founder_form a lemon:Form ;
  lemon:writtenRep "founder"@en .

:founder_of_nounpp a lexinfo:NounPPFrame ;
  lexinfo:copulativeArg :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:founder_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :founder_sense_ontomap ;
  lemon:reference <http://dbpedia.org/ontology/founder>;
  lemon:subjOfProp :arg2 ;
  lemon:objOfProp :arg1 ;
  lemon:condition :founder_condition .

:founder_condition a lemon:condition ;
  lemon:propertyDomain <http://www.w3.org/2002/07/owl#Thing> ;
  lemon:propertyRange <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a lemon:SynRoleMarker ;
  lemon:canonicalForm [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
