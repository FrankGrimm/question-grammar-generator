@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_de a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :produzent_von ;
  lemon:entry    :von .

:produzent_von a         lemon:LexicalEntry ;
  lemon:language       "de" ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :produzent_form ;
  lemon:otherForm      :produzenten_form ;
  lemon:synBehavior    :produzent_von_nounpp ;
  lemon:sense          :produzent_sense_ontomap .

:produzent_form a   lemon:Form ;
  lemon:writtenRep "Produzent"@de .

:produzenten_form a  lemon:Form ;
  lemon:writtenRep "Produzenten"@de ;
  lexinfo:number   lexinfo:plural .

:produzent_von_nounpp a          lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:produzent_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping       :produzent_sense_ontomap ;
  lemon:reference         <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp        :arg1 ;
  lemon:objOfProp         :arg2 ;
  lemon:condition         :produzent_condition .

:produzent_condition a  lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :von .

## Prepositions ##

:von a                 lemon:Word ;
  lemon:language       "de" ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
