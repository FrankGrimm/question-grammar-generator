@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_de a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :komponist_fuer ;
  lemon:entry    :fuer .

:komponist_fuer a    lemon:LexicalEntry ;
  lemon:language       "de" ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :komponist_form ;
  lemon:otherForm      :komponisten_form ;
  lemon:synBehavior    :komponist_fuer_nounpp ;
  lemon:sense          :komponist_sense_ontomap .

:komponist_form a lemon:Form ;
  lemon:writtenRep    "Komponist"@de .

:komponisten_form a lemon:Form ;
  lemon:writtenRep     "Komponisten"@de ;
  lexinfo:number       lexinfo:plural .

:komponist_fuer_nounpp a     lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:komponist_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping            :komponist_sense_ontomap ;
  lemon:reference              <http://dbpedia.org/ontology/musicComposer> ;
  lemon:subjOfProp             :arg2 ;
  lemon:objOfProp              :arg1 ;
  lemon:condition              :komponist_condition .

:komponist_condition a lemon:condition ;
  lemon:propertyDomain     <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange      <http://dbpedia.org/ontology/MusicalArtist> .

:arg2 lemon:marker :fuer .

## Prepositions ##

:fuer a                 lemon:Word ;
  lemon:language       "de" ;
  lemon:canonicalForm  [ lemon:writtenRep "f√ºr"@de ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
