@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :TdeathPlace_of ;
  lemon:entry    :of .

:TdeathPlace_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :TdeathPlace_of_form ;
  lemon:synBehavior    :TdeathPlace_of_nounpp ;
  lemon:sense          :TdeathPlace_of_sense_ontomap .

:TdeathPlace_of_form a lemon:Form ;
  lemon:writtenRep "death place"@en .

:TdeathPlace_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:TdeathPlace_of_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :TdeathPlace_of_sense_ontomap ;
  lemon:reference           <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :TdeathPlace_of_condition .

:TdeathPlace_of_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .