@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_complete_on ;
  lemon:entry    :in .

:to_complete_on a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_complete_on ;
  lemon:otherForm      :form_complete_on_past ;
  lemon:synBehavior    :complete_on_frame ;
  lemon:sense          :complete_on_ontomap .

:form_complete_on a           lemon:Form ;
  lemon:writtenRep     "release"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_complete_on_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .



:complete_on_frame a                  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :complete_on_subj ;
  lexinfo:prepositionalAdjunct :complete_on_obj .

:complete_on_ontomap a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :complete_on_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp  :complete_on_obj ;
  lemon:objOfProp   :complete_on_subj ;
  lemon:condition   :complete_on_condition .


:complete_on_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:complete_on_obj lemon:marker :in .

## Prepositions ##

:in a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

