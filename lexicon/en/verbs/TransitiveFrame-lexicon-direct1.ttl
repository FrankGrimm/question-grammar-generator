@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :direct1 ;
  lemon:entry    :direct1ed ;
  lemon:entry    :by .

:direct1 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_direct1 ;
  lemon:otherForm      :form_direct1s ;
  lemon:otherForm      :form_direct1ed ;
  lemon:synBehavior    :direct1_frame_transitive ;
  lemon:sense          :direct1_ontomap .


:form_direct1 a         lemon:Form ;
  lemon:writtenRep     "direct"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_direct1s a    lemon:Form ;
  lemon:writtenRep "directs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_direct1ed a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:past .

:direct1_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :direct1_subj ;
  lexinfo:directObject     :direct1_obj .

:direct1_ontomap a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :direct1_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :direct1_obj ;
  lemon:objOfProp   :direct1_subj ;
  lemon:condition   :direct1_condition .


:direct1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:direct1ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_direct1ed_canonical ;
  lemon:otherForm      :form_direct1ed_by ;
  lemon:synBehavior    :direct1ed_frame_adjectivepp ;
  lemon:sense          :direct1ed_ontomap .

:form_direct1ed_canonical a lemon:Form ;
  lemon:writtenRep         "directed"@en .

:form_direct1ed_by a    lemon:Form ;
  lemon:writtenRep     "directed"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:direct1ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :direct1ed_subj ;
  lexinfo:prepositionalAdjunct :direct1ed_obj .

:direct1ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :direct1ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :direct1ed_subj ;
  lemon:objOfProp   :direct1ed_obj ;
  lemon:condition   :direct1_condition .

:direct1ed_obj lemon:marker :by .

## Prepositions ##

:by a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
