@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_join_intransitive ;
  lemon:entry    :joined ;
  lemon:entry    :to .


:to_join_intransitive a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_join ;
  lemon:otherForm      :form_joins ;
  lemon:otherForm      :form_joined ;
  lemon:synBehavior    :join_frame_intransitive ;
  lemon:sense          :join_sense1 .


:form_join a         lemon:Form ;
  lemon:writtenRep     "join"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_joins a         lemon:Form ;
  lemon:writtenRep     "joins"@en ;
  lexinfo:verbFormMood lexinfo:secondPerson .

:form_joined a    lemon:Form ;
  lemon:writtenRep "joined"@en ;
  lexinfo:tense    lexinfo:past .

:join_frame_intransitive a lexinfo:IntransitiveFrame ;
  lexinfo:subject          :join_subj ;
  lexinfo:directObject     :join_obj .

:join_sense1 a    lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :join_sense1 ;
  lemon:reference   <http://dbpedia.org/property/accessioneudate> ;
  lemon:subjOfProp  :join_obj ;
  lemon:objOfProp   :join_subj ;
  lemon:condition   :join_condition_country .

:join_condition_country a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/date> .



:joined a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_joined_canonical ;
#  lemon:synBehavior    :joined_frame_adjectivepp ;
  lemon:sense          :joined_sense1 .

:form_joined_canonical a lemon:Form ;
  lemon:writtenRep        "joined"@en .

:form_joined_in a     lemon:Form ;
  lemon:writtenRep     "joined"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:joined_sense1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :joined_sense1 ;
  lemon:reference   <http://dbpedia.org/property/accessioneudate> ;
  lemon:subjOfProp  :joined_subj ;
  lemon:objOfProp   :joined_obj ;
  lemon:condition   :join_condition_country .



