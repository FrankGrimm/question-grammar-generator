@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_join_transitive ;
#  lemon:entry    :to_join_intransitive ;
  lemon:entry    :joined .

:to_join_transitive a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_join ;
  lemon:otherForm      :form_joins ;
  lemon:otherForm      :form_joined ;
  lemon:synBehavior    :join_frame_transitive ;
  lemon:sense          :join_sense1 .


:form_join a         lemon:Form ;
  lemon:writtenRep     "join"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_joins a         lemon:Form ;
  lemon:writtenRep     "joins"@en ;
  lexinfo:verbFormMood lexinfo:secondPerson .

:form_joined a    lemon:Form ;
  lemon:writtenRep "joined"@en ;
  lexinfo:tense    lexinfo:past .

:join_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :join_subj ;
  lexinfo:directObject     :join_obj .

:join_sense1 a    lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :join_sense1 ;
  lemon:reference   <http://dbpedia.org/property/accessioneudate> ;
  lemon:subjOfProp  :join_obj ;
  lemon:objOfProp   :join_subj ;
  lemon:condition   :join_condition_country .

:join_condition_country a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .


